#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nrwl+tao@17.3.2_@swc+core@1.11.24/node_modules/@nrwl/tao/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nrwl+tao@17.3.2_@swc+core@1.11.24/node_modules/@nrwl/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nrwl+tao@17.3.2_@swc+core@1.11.24/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nrwl+tao@17.3.2_@swc+core@1.11.24/node_modules/@nrwl/tao/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nrwl+tao@17.3.2_@swc+core@1.11.24/node_modules/@nrwl/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nrwl+tao@17.3.2_@swc+core@1.11.24/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nrwl/tao/index.js" "$@"
else
  exec node  "$basedir/../@nrwl/tao/index.js" "$@"
fi
