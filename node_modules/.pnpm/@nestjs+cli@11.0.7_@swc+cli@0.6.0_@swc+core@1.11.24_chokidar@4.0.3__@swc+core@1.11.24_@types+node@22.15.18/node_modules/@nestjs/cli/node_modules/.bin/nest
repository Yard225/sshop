#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nestjs+cli@11.0.7_@swc+cli@0.6.0_@swc+core@1.11.24_chokidar@4.0.3__@swc+core@1.11.24_@types+node@22.15.18/node_modules/@nestjs/cli/bin/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nestjs+cli@11.0.7_@swc+cli@0.6.0_@swc+core@1.11.24_chokidar@4.0.3__@swc+core@1.11.24_@types+node@22.15.18/node_modules/@nestjs/cli/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nestjs+cli@11.0.7_@swc+cli@0.6.0_@swc+core@1.11.24_chokidar@4.0.3__@swc+core@1.11.24_@types+node@22.15.18/node_modules/@nestjs/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nestjs+cli@11.0.7_@swc+cli@0.6.0_@swc+core@1.11.24_chokidar@4.0.3__@swc+core@1.11.24_@types+node@22.15.18/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nestjs+cli@11.0.7_@swc+cli@0.6.0_@swc+core@1.11.24_chokidar@4.0.3__@swc+core@1.11.24_@types+node@22.15.18/node_modules/@nestjs/cli/bin/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nestjs+cli@11.0.7_@swc+cli@0.6.0_@swc+core@1.11.24_chokidar@4.0.3__@swc+core@1.11.24_@types+node@22.15.18/node_modules/@nestjs/cli/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nestjs+cli@11.0.7_@swc+cli@0.6.0_@swc+core@1.11.24_chokidar@4.0.3__@swc+core@1.11.24_@types+node@22.15.18/node_modules/@nestjs/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/@nestjs+cli@11.0.7_@swc+cli@0.6.0_@swc+core@1.11.24_chokidar@4.0.3__@swc+core@1.11.24_@types+node@22.15.18/node_modules:/Users/mac/Documents/DEV/NESTJS/sshop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nest.js" "$@"
else
  exec node  "$basedir/../../bin/nest.js" "$@"
fi
